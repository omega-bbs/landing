{
  "name": "@omega-bbs/landing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf ./public",
    "dev": "gatsby develop",
    "test": "npm run lint",
    "lint": "prettier --list-different \"./**/*.{js,css}\"",
    "prettier": "prettier --write \"./**/*.{js,css}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^1.9.50",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-react-helmet": "^1.0.6"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.2"
  }
}
